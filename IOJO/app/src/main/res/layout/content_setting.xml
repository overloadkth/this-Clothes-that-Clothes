<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    tools:context="mju.com.iojo.Setting.SettingActivity"
    tools:showIn="@layout/activity_setting">

    <Button
        android:id="@+id/NotiButton"
        android:layout_width="match_parent"
        style="?android:attr/borderlessButtonStyle"
        android:drawableLeft="@drawable/ic_notifications_black_24dp"
        android:layout_height="50dp"
        android:text="알림 설정" />

    <Button
        android:id="@+id/BookmarkButton"
        android:layout_width="match_parent"
        style="?android:attr/borderlessButtonStyle"
        android:drawableLeft="@drawable/ic_star_black_24dp"
        android:layout_height="50dp"
        android:text="즐겨찾기 관리"
        android:layout_below="@+id/NotiButton" />

</RelativeLayout>
